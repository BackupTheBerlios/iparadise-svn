<ejb-jar>
    <enterprise-beans>

        <session>
            <ejb-name>EJBInvocatorSessionEJB</ejb-name>
            <home>com.pdpsoft.business.sessionbean.EJBInvocatorSessionHome</home>
            <remote>com.pdpsoft.business.sessionbean.EJBInvocatorSession</remote>
            <local-home>com.pdpsoft.business.sessionbean.LocalEJBInvocatorSessionHome</local-home>
            <local>com.pdpsoft.business.sessionbean.LocalEJBInvocatorSession</local>
            <ejb-class>com.pdpsoft.business.sessionbean.EJBInvocatorSessionBean</ejb-class>
            <session-type>Stateless</session-type>
