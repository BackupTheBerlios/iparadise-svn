<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.apsg.samane.g16.crm.ShipmentPackageEntity" table="SHIPMENT_PACKAGE">
        <id name="shipmentPackageId" type="java.lang.Integer" column="SHIPMENT_PACKAGE_ID">
            <generator class="increment"/>
        </id>
        <property name="dateCreated" type="java.util.Date">
            <column name="DATE_CREATED"/>
        </property>
        <property name="boxNumber" type="java.lang.String">
            <column name="BOX_NUMBER"/>
        </property>

    </class>
</hibernate-mapping>