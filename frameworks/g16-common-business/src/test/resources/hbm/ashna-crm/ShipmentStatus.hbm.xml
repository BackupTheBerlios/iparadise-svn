<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.apsg.samane.g16.crm.ShipmentStatus" table="SHIPMENT_STATUS">
        <id name="shipmentStatusId" type="java.lang.Integer" column="SHIPMENT_STATUS_ID">
            <generator class="increment"/>
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" />
        </property>

    </class>
</hibernate-mapping>