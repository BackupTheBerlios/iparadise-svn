<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.apsg.samane.g16.sabt.RankingEntity" table="SBT_RANKING">
        <id name="rankingId" type="java.lang.Integer" column="RANKING_ID">
            <generator class="increment"/>
        </id>
        <property name="title" type="java.lang.String">
            <column name="TITLE"/>
        </property>
        <property name="description" type="java.lang.String">
            <column name="DESCRIPTION"/>
        </property>
        <property name="rankingNumber" type="java.lang.Float">
            <column name="RANK_NO"/>
        </property>

    </class>
</hibernate-mapping>